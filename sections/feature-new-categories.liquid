                        src="{{ image | img_url: '50x50' }}"
                        data-src="{{ img_url }}"
                        data-widths="[157, 270, 303, 370, 377, 670, 570, 720, 850]"
                        data-aspectratio="{{ image.aspect_ratio }}"
                        data-sizes="auto"
                        class="lazyload w__100 promo-banner-image image-1 desk"
                        alt="{{image.alt}}"
                      >
                      <img
                        src="{{ image_mob | img_url: '50x50' }}"
                        data-src="{{ image_mob }}"
                        data-widths="[157, 270, 303, 370, 377, 670, 570, 720, 850]"
                        data-aspectratio="{{ image.aspect_ratio }}"
                        data-sizes="auto"
                        class="lazyload w__100 promo-banner-image image-1 mobile"
                        alt="{{image.alt}}"
                      >
