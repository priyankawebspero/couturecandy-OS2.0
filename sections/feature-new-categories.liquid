              class="promo-banner gl_col-md-6 gl_col-lg-6"
              onclick="window.location.href='{{ block.settings.banner_link }}'"
            >
              <div class="promo-banner-inner">
                <div class="main-wrapp-img gl_col-md-6 gl_col-lg-6">
                  <div class="banner-image">
                    {%- if block.settings.image == blank -%}
                      <img
                        class="promo-banner-image image-1"
                        src="//cdn.shopify.com/s/files/1/1933/6253/files/portfolio-placeholder_600x400_crop_center.jpg?12790048884060573015"
                        width="600"
                        height="420"
                        alt="market-banner-sport"
                        title="market-banner-sport"
                      >
                    {%- else -%}
                      {%- assign image = block.settings.image -%}
                      {%- assign image_mob = block.settings.image_mob -%}
                      {%- assign img_url = image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
                      {%- assign image_mob = image_mob | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
